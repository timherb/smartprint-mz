{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Main Process",
      "type": "node",
      "request": "attach",
      "port": 5858,
      "restart": true,
      "sourceMaps": true,
      "outFiles": ["${workspaceFolder}/out/main/**/*.js"],
      "resolveSourceMapLocations": ["${workspaceFolder}/out/**"],
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
